  "author": "Alexandre Louveau <alexandre.louveau31@gmail.com>",
  "keywords": [
    "vue",
    "vue-reorderable-grid",
    "vue-reorderable-layout",
    "vue-reorderable-dashboard"
  ],
  "license": "MIT",
  "repository": {
